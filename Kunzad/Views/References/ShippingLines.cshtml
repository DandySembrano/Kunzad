@{
    Layout = "~/Views/Shared/_DataTableLayout3.cshtml";
}

<!-- Shipping Line List/DataTable -->
@section tableContent{
    <div class="row">
        <div class="col-lg-12">
            <div class="myTableContainter">
                <div class="myTableContent">
                    <!--Header-->
                    <div class="myTableRow-header text-dark">
                        <div class="myTableCell cell-seqno small text-center">
                            <strong>No.</strong>
                        </div>
                        <div class="myTableCell cell-10 small" ng-click="processSLOrderBy('Name')">
                            <strong>Name</strong>
                            <span class="glyphicon glyphicon-chevron-down"
                                  ng-show="orderBySLNameDesc"></span>
                            <span class="glyphicon glyphicon-chevron-up"
                                  ng-show="orderBySLNameAsc"></span>
                        </div>
                    </div>
                    <!--Detail-->
                    <div class="myTableRow"
                         ng-repeat="s in shippingLineList | orderBy:slCriteria"
                         ng-click="setSelected($index, s.Id)"
                         context-menu="setSelected($index, s.Id)"
                         data-target="DataTableMenu">
                        <div class="myTableCell cell-seqno small text-center">{{currentPage == 1 ? ($index + 1) : (($index + 1) + ((currentPage - 1) * pageSize))}}</div>
                        <div class="myTableCell cell-10 small">{{s.Name}}</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<!-- Shipping Line/General Form -->
@section tab1Content{
    <div class="row" ng-show="selectedTab === tabPages[0]">
        <div class="alert alert-danger alert-sm" ng-show="isError">
            <i class="fa fa-remove pr10"></i>
            <strong>ERROR!</strong>    {{errorMessage}}
        </div>
        <div class="col-lg-6">
            <form class="form-horizontal" role="form">
                <div class="form-group form-group-sm">
                    <label class="col-md-3 control-label small">Name</label>
                    <div class="col-md-8">
                        <input type="text"
                               class="form-control"
                               placeholder=""
                               ng-model="shippingLineItem.Name"
                               ng-disabled="viewOnly"
                               pattern=".{0,150}"
                               ng-change="initializeHeaderName()">
                    </div>
                </div>
            </form>
        </div>
    </div>
}

<!-- Vessel Form-->
@section tab2Content{
<div class="row" ng-show="selectedTab === tabPages[1] ">
    <div class="col-lg-12">
        <div class="row">
            <div class="padding-right-30">
                <button class="btn btn-sm pull-right" ng-click="openVesselForm('Create')">
                    <span class="glyphicon glyphicon-plus"></span>
                </button>
            </div>
        </div><br />
        <div class="myTableContainter">
            <div class="myTableContent">
                <!--Header-->
                <div class="myTableRow-header text-dark" ng-show="vesselList.length > 0">
                    <div class="myTableCell cell-seqno small text-center"><strong>No.</strong></div>
                    <div class="myTableCell cell-8 small" ng-click="processVesselOrderBy('Name')">
                        <strong>Name</strong>
                        <span class="glyphicon glyphicon-chevron-down"
                              ng-show="orderByVesselNameDesc"></span>
                        <span class="glyphicon glyphicon-chevron-up"
                              ng-show="orderByVesselNameAsc"></span>
                    </div>
                    <div class="myTableCell cell-2 text-right small"><strong>Show/Edit/Delete</strong></div>
                </div>
                <!--Vessel List-->
                <div class="myTableRow"
                     ng-repeat="item in paginatedVessels | orderBy:vesselCriteria">
                    <div class="myTableCell cell-seqno small text-center">{{vesselCurrentPage == 1 ? ($index + 1) : (($index + 1) + ((vesselCurrentPage - 1) * vesselPageSize))}}</div>
                    <div class="myTableCell cell-8 small">{{item.Name}}</div>
                    <div class="myTableCell cell-2 small text-right">
                        <button class="btn btn-xs btn-success light" type="button"
                                title="Add/Edit Voyage"
                                ng-click="openVesselForm('showVoyage', item.Id, item.Name)">
                            Voyages
                        </button>
                        <button title="Edit Vessel" class="btn btn-xs btn-primary light" type="button" ng-click="openVesselForm('Edit', item.Id)">
                            <span class="glyphicon glyphicon-pencil"></span>
                        </button>
                        <button title="Delete Vessel" class="btn btn-xs btn-danger light" type="button" ng-click="openVesselForm('Delete', item.Id)">
                            <span class="glyphicons glyphicons-bin"></span>
                        </button>
                    </div>
                </div>
            </div>
        </div><br />
        <!--Pagination Buttons-->
        <div class="myTableContainter" ng-show="paginatedVessels.length > 0">
            <div class="myTableContent">
                <div class="myTableRow-no-border text-right">
                    <div class="btn-group btn-group-sm">
                        @*<div class="myTableCell-no-padding text-right small">
                            <button class="btn btn-primary btn-sm"
                                    type="button"
                                    ng-click="processVesselPagination(vesselCurrentPage = 1, '')"
                                    ng-disabled="!firstPageVessels">
                                First
                            </button>
                        </div>*@
                        <div class="myTableCell-no-padding text-right small">
                            <button class="btn btn-primary btn-sm"
                                    type="button"
                                    ng-click="processVesselPagination(vesselCurrentPage = (vesselCurrentPage - 1), '')"
                                    ng-disabled="!previousPageVessels">
                                Previous
                            </button>
                        </div>
                        <div class="myTableCell-no-padding text-right small">
                            <button class="btn btn-default btn-sm"
                                    type="button"
                                    ng-disabled="true">
                                {{ vesselCurrentPage }}
                            </button>
                        </div>
                        <div class="myTableCell-no-padding text-right small">
                            <button class="btn btn-primary btn-sm"
                                    type="button"
                                    ng-click="processVesselPagination(vesselCurrentPage = (vesselCurrentPage + 1), '')"
                                    ng-disabled="!nextPageVessels">
                                Next
                            </button>
                        </div>
                        @*<div class="myTableCell-no-padding text-right small">
                            <button class="btn btn-primary btn-sm"
                                    type="button"
                                    ng-click="processVesselPagination(vesselCurrentPage = vesselMaxPage, '')"
                                    ng-disabled="!lastPageVessels">
                                Last
                            </button>
                        </div>*@
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
}

